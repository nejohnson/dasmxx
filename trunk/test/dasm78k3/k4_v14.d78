# Load boot ROM version 1.4
fk4_v14.bin

# Pull in CPU definitions
i78310_regs.d78



n0000
#----------------------------------------------------------
#
#    Memory Map:
#
#    FFFF  +---------------------------------+
#          | DEC - hardware blocks           |
#    E000  +---------------------------------+
#          | RAM                             |
#    C000  +---------------------------------+
#          | RAM/CARD/ROM                    |
#    8000  +---------------------------------+
#          | ROM                             |
#    0000  +---------------------------------+
#
#----------------------------------------------------------
.

n0040
#----------------------------------------------------------
# Function:
#       System_Start
#
# Args:
#       none
#
# Returns:
#       none
#
# Uses:
#       lots
#
# Notes:
#       Start of day from reset, brings up system.
#----------------------------------------------------------
.
p0040 System_Start
k0045 Set stack to top of RAM
l0071 Init_Loop_1
l007f Wait_Loop_1
l008a Wait_Loop_2
l00a5 L_4
l0132 L_5
#l014b L_6
#l0149 L_7
l00da Init_Loop_3
l0108 Init_Loop_4
l0124 main_loop




n0181
#----------------------------------------------------------
#
# Empty interrupt handler, does nothing just re-enable
# interrupts and return.
#
#----------------------------------------------------------
.
p0181 Dummy_Int_Handler



p022d EXIF0_Handler

p01b9 TMR0_Handler

p020c CRF01_Handler

p0183 SEF_Handler

p0195 SRF_Handler

p01a7 STF_Handler






n0170
#----------------------------------------------------------
# Function:
#       Delay_Timer
#
# Args:
#       C = number of delays (n x 0.98s)
#
# Returns:
#       none
#
# Uses:
#       Timer 0
#
# Notes:
#     Assumes 12MHz input clock, fCLK = 6MHz
#----------------------------------------------------------
.
p0170 Delay_Timer
k0170 Set count to 45875
k0174 Start count, clk=fCLK/128
l0177 l_delay_loop
k0177 Spin until counted down
k017b Clear count-end flag
k017e Loop for C cycles



n1375
#----------------------------------------------------------
# Function:
#       MemClear
#
# Args:
#       HL = Pointer to memory to clear
#       C  = Number of bytes to clear (0x01 .. 0xFF)
#
# Returns:
#       none
#
# Uses:
#       A
#
# Notes:
#----------------------------------------------------------
.
p1375 MemClear
l1377 l_memclr



